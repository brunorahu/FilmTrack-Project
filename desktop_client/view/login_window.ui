  <property name="styleSheet">
   <string notr="true">/* ----- 1. Ajustes Generales ----- */
QDialog {
    background-color: #1e1e1e; /* Fondo general */
}

/* ¡AQUÍ ESTÁ LA SOLUCIÓN! */
/* Forzamos a TODOS los QFrame a ser transparentes por defecto */
QFrame {
    background-color: transparent;
    border: none;
}

/* ----- 2. Encabezado &quot;FilmTrack&quot; (frame_2) ----- */
QFrame#frame_2 {
    background-color: #111111; /* Barra superior negra */
}
QLabel#label_filmtrack {
    color: white;
    font-size: 16px;
    font-weight: bold;
    padding: 10px;
}

/* ----- 3. Título &quot;Bienvenido&quot; (header_frame_2) ----- */
/* (Ya no necesita estilo de frame, será transparente por defecto) */
QLabel#label_bienvenido {
    color: white;
    font-size: 36px;
    font-weight: bold;
}

/* ----- 4. Contenedores de Login (frame_4 y frame_5) ----- */
/* (El frame_3 que los contiene será transparente por defecto) */
QFrame#frame_4, QFrame#frame_5 {
    background-color: #2b2b2b; /* Cajas de login gris oscuro */
    border: 1px solid #444;
    border-radius: 8px;
}

/* ----- 5. Etiquetas &quot;Sign In&quot; y &quot;Sign Up&quot; ----- */
QLabel#label_sign_in, QLabel#label_sign_up {
    color: white;
    font-size: 20px;
    font-weight: bold;
}

/* ----- 6. Campos de Texto (QLineEdit) ----- */
QLineEdit {
    background-color: #3c3c3c;
    color: #f0f0f0;
    border: 1px solid #555;
    border-radius: 5px;
    padding: 12px;
    font-size: 14px;
}
QLineEdit:focus {
    border: 1px solid #4a90e2;
}

/* ----- 7. Botones (QPushButton) ----- */
QPushButton#btn_login {
    background-color: white;
    color: black;
    border-radius: 5px;
    padding: 12px;
    font-size: 14px;
    font-weight: bold;
}
QPushButton#btn_login:hover {
    background-color: #f0f0f0;
}

QPushButton#btn_create_account {
    background-color: transparent;
    color: white;
    border: 2px solid white;
    border-radius: 5px;
    padding: 12px;
    font-size: 14px;
    font-weight: bold;
}
QPushButton#btn_create_account:hover {
    background-color: #333333;
}

/* ----- Estilo para Ventanas de Aviso ----- */
QMessageBox {
    background-color: #2b2b2b; /* Fondo oscuro */
}

/* Etiqueta de texto dentro del aviso */
QMessageBox QLabel {
    color: #f0f0f0; /* Texto blanco */
    font-size: 14px;
}

/* Botones (OK, Cancelar) dentro del aviso */
QMessageBox QPushButton {
    background-color: #4a90e2; /* Botón azul */
    color: white;
    border: none;
    border-radius: 4px;
    padding: 10px;
    font-size: 14px;
    font-weight: bold;
    min-width: 80px; /* Un ancho mínimo para que el botón OK se vea bien */
}
QMessageBox QPushButton:hover {
    background-color: #5aa1f2;
}</string>
